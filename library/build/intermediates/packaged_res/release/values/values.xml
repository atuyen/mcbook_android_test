    <declare-styleable name="CustomTabmoreAttrs">
        <attr format="integer" name="imageIcon"/>
        <attr format="string" name="txtTextTabMore"/>
    </declare-styleable>
    <declare-styleable name="ExtAutoCompleteTextView">
        <attr format="string" name="autoCompleteFontAssetName"/>
    </declare-styleable>
    <declare-styleable name="ExtButton">
        <attr format="string" name="buttonFontAssetName"/>
    </declare-styleable>
    <declare-styleable name="ExtCheckBox">
        <attr format="string" name="checkBoxFontAssetName"/>
    </declare-styleable>
    <declare-styleable name="ExtEditText">
        <attr name="validate">
            <flag name="regexp" value="0x0001"/>
            <flag name="numeric" value="0x0002"/>
            <flag name="alpha" value="0x0004"/>
            <flag name="alphaNumeric" value="0x0008"/>
            <flag name="email" value="0x0010"/>
            <flag name="creditCard" value="0x0020"/>
            <flag name="phone" value="0x0040"/>
            <flag name="domainName" value="0x0080"/>
            <flag name="ipAddress" value="0x0100"/>
            <flag name="webUrl" value="0x0200"/>
            <flag name="nocheck" value="0"/>
        </attr>
        <attr format="string" name="errorString"/>
        <attr format="string" name="emptyErrorString"/>
        <attr format="string" name="validatorRegexp"/>
        <attr format="boolean" name="isEmpty"/>
        <attr format="string" name="editTextFontAssetName"/>
    </declare-styleable>
    <declare-styleable name="ExtPagingListView">
        <attr format="boolean" name="swipeRefreshEnable"/>
        <attr format="color" name="swipeRefreshIndicatorColor"/>
        <attr format="boolean" name="scrollbarVisible"/>
        <attr format="color" name="dividerColor"/>
        <attr format="dimension" name="dividerHeight"/>
        <attr format="string" name="noDataMessage"/>
    </declare-styleable>
    <declare-styleable name="ExtRadioButton">
        <attr format="string" name="radioButtonFontAssetName"/>
    </declare-styleable>
    <declare-styleable name="ExtReadMoreTextView">
        <attr format="string" name="trimExpandedText"/>
        <attr format="string" name="trimCollapsedText"/>
        <attr format="integer" name="trimLength"/>
        <attr format="boolean" name="showTrimExpandedText"/>
        <attr format="color" name="colorClickableText"/>
    </declare-styleable>
    <declare-styleable name="ExtSwitchView">
        <attr format="string" name="switchFontAssetName"/>
    </declare-styleable>
    <declare-styleable name="ExtTextView">
        <attr format="string" name="textViewFontAssetName"/>
        <attr format="integer" name="limit"/>
        <attr format="boolean" name="isLimit"/>
        <attr format="boolean" name="chooseItem"/>
    </declare-styleable>
